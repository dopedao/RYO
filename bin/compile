#!/bin/bash
set -eu


starknet-compile contracts/GameEngineV1.cairo \
    --output artifacts/compiled/GameEngineV1.json \
    --abi artifacts/abi/GameEngineV1_abi.json

starknet-compile contracts/MarketMaker.cairo \
    --output artifacts/compiled/MarketMaker.json \
    --abi artifacts/abi/MarketMaker_abi.json

starknet-compile contracts/UserRegistry.cairo \
    --output artifacts/compiled/UserRegistry.json \
    --abi artifacts/abi/UserRegistry_abi.json

starknet-compile contracts/Account.cairo \
    --output artifacts/compiled/Account.json \
    --abi artifacts/abi/Account_abi.json

starknet-compile contracts/Combat.cairo \
    --output artifacts/compiled/Combat.json \
    --abi artifacts/abi/Combat_abi.json